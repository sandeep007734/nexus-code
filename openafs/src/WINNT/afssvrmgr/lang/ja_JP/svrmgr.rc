MENU_MAIN MENU DISCARDABLE 
BEGIN
    POPUP "セル(&C)"
    BEGIN
        MENUITEM "選択(&S)...",                 M_CELL_OPEN
        MENUITEM "認証(&A)...",                 M_CREDENTIALS
        MENUITEM SEPARATOR
        MENUITEM "終了(&X)",                    M_EXIT
    END
    POPUP "表示(&V)"
    BEGIN
        POPUP "サーバー(&S)"
        BEGIN
            MENUITEM "大アイコン(&G)",              M_SVR_VIEW_LARGE
            , CHECKED
            MENUITEM "小アイコン(&M)",              M_SVR_VIEW_SMALL
            , CHECKED
            MENUITEM "詳細(&D)",                    M_SVR_VIEW_REPORT
            , CHECKED
            MENUITEM SEPARATOR
            MENUITEM "標準(&N)",                    M_SVR_VIEW_ONEICON
            , CHECKED
            MENUITEM "すべてのアイコン(&A)",        M_SVR_VIEW_TWOICONS
            , CHECKED
            MENUITEM "状況のみ(&S)",                M_SVR_VIEW_STATUS
            , CHECKED
        END
        POPUP "クィック・ビュー・ペイン(&Q)"
        BEGIN
            MENUITEM "なし(&N)",                    M_DIVIDE_NONE, CHECKED
            MENUITEM "縦に分割(&V)",                M_DIVIDE_H, CHECKED
            MENUITEM "横に分割(&H)",                M_DIVIDE_V, CHECKED
        END
        MENUITEM SEPARATOR
        MENUITEM "進行中の操作(&P)",            M_ACTIONS, CHECKED
        MENUITEM SEPARATOR
        MENUITEM "モニターされるサーバー(&M)...", M_SUBSET
        MENUITEM "列(&C)...",                   M_COLUMNS
        MENUITEM "オプション(&O)...",           M_OPTIONS
        MENUITEM SEPARATOR
        MENUITEM "すべて最新表示(&F)",          M_REFRESHALL
    END
    POPUP "ヘルプ(&H)"
    BEGIN
        MENUITEM "目次(&C)",                    M_HELP
        MENUITEM "コマンド検索(&F)...",         M_HELP_FIND
