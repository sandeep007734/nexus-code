            , CHECKED
            MENUITEM "상태만(&S)",                  M_SVR_VIEW_STATUS
            , CHECKED
        END
        POPUP "빨리 보기 분할영역(&Q)"
        BEGIN
            MENUITEM "없음(&N)",                    M_DIVIDE_NONE, CHECKED
            MENUITEM "수직 분할(&V)",               M_DIVIDE_H, CHECKED
            MENUITEM "수평 분할(&H)",               M_DIVIDE_V, CHECKED
        END
        MENUITEM SEPARATOR
        MENUITEM "진행 중인 조작(&P)",          M_ACTIONS, CHECKED
