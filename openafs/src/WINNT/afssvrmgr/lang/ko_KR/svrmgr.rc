        MENUITEM SEPARATOR
        MENUITEM "종료(&X)",                    M_EXIT
    END
    POPUP "보기(&V)"
    BEGIN
        POPUP "서버(&S)"
        BEGIN
            MENUITEM "큰 아이콘(&G)",               M_SVR_VIEW_LARGE
            , CHECKED
            MENUITEM "작은 아이콘(&M)",             M_SVR_VIEW_SMALL
            , CHECKED
